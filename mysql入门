  `users_name` varchar(20) NOT NULL COMMENT '用户名',
  `email` varchar(50) NOT NULL COMMENT '用户邮箱',
  `age` tinyint(3) unsigned NOT NULL COMMENT '用户年龄',
  `fee` decimal(10,2) NOT NULL DEFAULT '0.00' COMMENT '用户余额',
